<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LogInView">
    <children>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <children>
                <TextField fx:id="Username" layoutX="180.0" layoutY="40.0" prefHeight="39.0" prefWidth="211.0" />
            </children>
        </AnchorPane>
        <Label fx:id="Password" layoutX="42.0" layoutY="86.0" prefHeight="45.0" prefWidth="140.0" styleClass="label" text="Enter Password:">
            <font>
                <Font name="Gabriola" size="24.0" />
            </font>
        </Label>
        <Label layoutX="40.0" layoutY="30.0" prefHeight="45.0" prefWidth="140.0" styleClass="label" text="Enter User Name:">
            <font>
                <Font name="Gabriola" size="22.0" />
            </font>
        </Label>
          <Button fx:id="btnLogIn" layoutX="435.0" layoutY="40.0" mnemonicParsing="false" onAction="#connect" prefHeight="25.0" prefWidth="94.0" text="Log In">
         <font>
            <Font name="Gabriola" size="18.0" />
         </font></Button>
      <CheckBox fx:id="saleVacation" layoutX="112.0" layoutY="220.0" mnemonicParsing="false" onAction="#updateSaleDbRequestForApproval" text="Check if you'd like to confirm the sale of your vacation">
         <font>
            <Font name="Gabriola" size="18.0" />
         </font>
      </CheckBox>
      <Button fx:id="btnContinue" layoutX="435.0" layoutY="91.0" mnemonicParsing="false" onAction="#Continue" prefHeight="35.0" prefWidth="94.0" text="continue">
         <font>
            <Font name="Gabriola" size="18.0" />
         </font>
      </Button>
      <PasswordField fx:id="UserPassword" layoutX="180.0" layoutY="91.0" prefHeight="25.0" prefWidth="211.0">
         <font>
            <Font size="18.0" />
         </font></PasswordField>
          <Button fx:id="backLogIn" layoutX="252.0" layoutY="344.0" mnemonicParsing="false" onAction="#backOriginStartWindow" prefHeight="25.0" prefWidth="109.0" text="Back">
         <font>
            <Font name="Gabriola" size="18.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
