<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Inbox.InboxView">
   <children>
      <Button fx:id="btnGetAllRequestsForPurchase" layoutX="37.0" layoutY="140.0" mnemonicParsing="false" onAction="#getAllRequestsForPurchase" styleClass="welcome" text="Get All Requests For Cash Purchase">
         <font>
            <Font name="Gabriola" size="18.0" />
         </font></Button>
      <Button fx:id="btnBack" layoutX="265.0" layoutY="327.0" mnemonicParsing="false" onAction="#backOrigin" styleClass="welcome" text="back">
         <font>
            <Font name="Gabriola" size="18.0" />
         </font></Button>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="156.0" layoutY="25.0" text="EveryVaction4U">
         <font>
            <Font name="Gabriola" size="64.0" />
         </font>
      </Label>
      <Button fx:id="btnGetAllRequestsForConversions" layoutX="289.0" layoutY="140.0" mnemonicParsing="false" onAction="#getAllRequestsForConversion" styleClass="welcome" text="Get All Requests for Conversions purchase">
         <font>
            <Font name="Gabriola" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnGetAllConfirmations" layoutX="311.0" layoutY="220.0" mnemonicParsing="false" onAction="#getAllConfirmations" styleClass="welcome" text="Get confirmations">
         <font>
            <Font name="Gabriola" size="18.0" />
         </font>
      </Button>
      <Button layoutX="156.0" layoutY="221.0" mnemonicParsing="false" onAction="#getDeclinesList" styleClass="welcome" text="Get Declines">
         <font>
            <Font name="Gabriola" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
